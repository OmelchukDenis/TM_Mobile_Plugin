{
	"manifest_version": 2,
	"name": "TM Mobile Plugin",
	"description": "Mobile Plugin for some actions",
	"version": "1.0.0",
	"icons": {"128": "icon_128_1.png"},
	"browser_action": {
		"default_icon": "icon_1.png",
		"default_popup": "index.html"
	},
    "permissions": ["cookies","activeTab", "https://getuikit.com"],
    "web_accessible_resources": [
        "app.js"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
}